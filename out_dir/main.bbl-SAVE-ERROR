      \strng{namehash}{38585876cf997384dab7a4e53a0fa5e7}
      \strng{fullhash}{38585876cf997384dab7a4e53a0fa5e7}
      \strng{fullhashraw}{38585876cf997384dab7a4e53a0fa5e7}
      \strng{bibnamehash}{38585876cf997384dab7a4e53a0fa5e7}
      \strng{authorbibnamehash}{38585876cf997384dab7a4e53a0fa5e7}
