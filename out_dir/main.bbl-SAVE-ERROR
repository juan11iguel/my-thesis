    \entry{song_novel_2021}{article}{}{}
      \name{author}{5}{}{%
        {{hash=c5f0e0a255be2b7271f89cca56418703}{%
           family={Song},
           familyi={S\bibinitperiod},
           given={Jialiang},
           giveni={J\bibinitperiod}}}%
        {{hash=c30c61ca819fc201c0b555d531a2c21e}{%
           family={Chen},
