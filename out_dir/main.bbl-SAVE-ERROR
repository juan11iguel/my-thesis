      \strng{bibnamehash}{a287989fa86e380432cde680453b76df}
      \strng{authorbibnamehash}{a287989fa86e380432cde680453b76df}
      \strng{authornamehash}{a287989fa86e380432cde680453b76df}
      \strng{authorfullhash}{d5d00d9f6623aac5cd454d8e0ccbcb09}
