    \entry{palenzuela_experimental_2022}{article}{}{}
      \name{author}{4}{}{%
        {{hash=11222ba11430c93aa5cb48269d9c41ad}{%
           family={Palenzuela},
           familyi={P\bibinitperiod},
           given={Patricia},
