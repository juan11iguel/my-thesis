    \entry{shahzad_standard_2019}{article}{}{}
      \name{author}{3}{}{%
        {{hash=379d94de656d91e2de557f62a139f4e1}{%
           family={Shahzad},
           familyi={S\bibinitperiod},
           given={Muhammad\bibnamedelima Wakil},
           giveni={M\bibinitperiod\bibinitdelim W\bibinitperiod}}}%
        {{hash=f6e0abf9bb55df7ad4aa93f3933b8675}{%
