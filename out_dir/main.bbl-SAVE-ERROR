    \entry{asvapoositkul_comparative_2014}{article}{}{}
      \name{author}{2}{}{%
        {{hash=2c760a3f273d138a25b3af9a3671d425}{%
           family={Asvapoositkul},
           familyi={A\bibinitperiod},
           given={Wanchai},
           giveni={W\bibinitperiod}}}%
        {{hash=befde5f640cfb66973758c560cbcf33d}{%
           family={Kuansathan},
           familyi={K\bibinitperiod},
           given={Mantheerapol},
