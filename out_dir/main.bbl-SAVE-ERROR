    \entry{binz_technologysensitive_2017}{article}{}{}
      \name{author}{4}{}{%
        {{hash=c65d67a677b6a525dcfd6606472f3a98}{%
           family={Binz},
           familyi={B\bibinitperiod},
           given={Christian},
