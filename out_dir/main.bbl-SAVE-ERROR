    \entry{sharqawy_formulation_2010}{article}{}{3}
      \name{author}{3}{}{%
        {{hash=98226c916a0eadc37375e74587a7a2d5}{%
           family={Sharqawy},
           familyi={S\bibinitperiod},
           given={Mostafa\bibnamedelima H.},
