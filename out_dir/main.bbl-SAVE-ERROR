      \strng{namehash}{59a5dd3665f4ed5022668573022f57a5}
      \strng{fullhash}{59a5dd3665f4ed5022668573022f57a5}
      \strng{fullhashraw}{59a5dd3665f4ed5022668573022f57a5}
      \strng{bibnamehash}{59a5dd3665f4ed5022668573022f57a5}
      \strng{authorbibnamehash}{59a5dd3665f4ed5022668573022f57a5}
