      \strng{namehash}{5725446e42c8bb572492328c687c2673}
      \strng{fullhash}{753b15a8e091fe43b2f241bef3cf948b}
      \strng{fullhashraw}{753b15a8e091fe43b2f241bef3cf948b}
      \strng{bibnamehash}{5725446e42c8bb572492328c687c2673}
      \strng{authorbibnamehash}{5725446e42c8bb572492328c687c2673}
      \strng{authornamehash}{5725446e42c8bb572492328c687c2673}
      \strng{authorfullhash}{753b15a8e091fe43b2f241bef3cf948b}
