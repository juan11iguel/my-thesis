    \entry{wolff_monte_2007}{article}{}{}
      \name{author}{1}{}{%
        {{hash=6aa8ea58f5e51071897db1d0e8cf7655}{%
           family={Wolff},
           familyi={W\bibinitperiod},
           given={Ulli},
