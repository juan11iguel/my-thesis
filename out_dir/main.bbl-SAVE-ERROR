    \entry{hosoz_performance_2007}{article}{}{}
      \name{author}{3}{}{%
        {{hash=0b6e6e546898a7ff7299bc0c50417c16}{%
           family={Hosoz},
           familyi={H\bibinitperiod},
           given={M.},
