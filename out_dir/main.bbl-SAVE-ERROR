        {{hash=d099b932f81d484885687ef04fda74cc}{%
           family={Ng},
           familyi={N\bibinitperiod},
           given={Kim\bibnamedelima Choom},
           giveni={K\bibinitperiod\bibinitdelim C\bibinitperiod}}}%
