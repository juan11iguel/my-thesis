    \entry{palenzuela_concentrating_2015}{book}{}{6}
      \name{author}{3}{}{%
        {{hash=11222ba11430c93aa5cb48269d9c41ad}{%
           family={Palenzuela},
           familyi={P\bibinitperiod},
           given={Patricia},
