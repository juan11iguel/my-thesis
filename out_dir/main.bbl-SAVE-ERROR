      \strng{namehash}{ba91a53e9f95d3eae93c788b37971d20}
      \strng{fullhash}{c476603c7d9e8d93917bd2d81fbcb57b}
      \strng{fullhashraw}{c476603c7d9e8d93917bd2d81fbcb57b}
      \strng{bibnamehash}{ba91a53e9f95d3eae93c788b37971d20}
      \strng{authorbibnamehash}{ba91a53e9f95d3eae93c788b37971d20}
      \strng{authornamehash}{ba91a53e9f95d3eae93c788b37971d20}
      \strng{authorfullhash}{c476603c7d9e8d93917bd2d81fbcb57b}
