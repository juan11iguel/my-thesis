    \entry{bouma_metrics_2020}{article}{}{}
      \name{author}{3}{}{%
        {{hash=c3e4b7057e6a345cb312eb92d1856c6e}{%
           family={Bouma},
           familyi={B\bibinitperiod},
           given={Andrew\bibnamedelima T.},
